package Array;

//√∞≈›≈≈–ÚÀ„∑®
public class MaoPao {

	public static void main(String[] args) {

	}

	public static void maopao() {
		int[] arr = { 7, 3, 12, 1, 0 };
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length - i; j++) {
				if (arr[j] > arr[j + 1]) {
					int temp = arr[j];
					arr[j] = arr[j + 1];
					arr[j + 1] = temp;
				}
			}
		}
	}

}
